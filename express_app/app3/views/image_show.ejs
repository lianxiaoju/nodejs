<script src="/javascripts/jquery.min.js" type="text/javascript"></script>
<script>
 	$("#send").click(function(){
 		$.ajax({
 			url:"/api_image_sever",
 			type:"get",
 			dataType:"json",
 			success : function(data) 
			{ 
				console.log(data)
				var datas=eval(data)
				console.log(datas[0].url)
				var imgs="",$a="";
				for (var i = 0; i < datas.length; i++) {
					console.log(datas[i])
					imgs+='<img src="'+datas[i].url+'" alt="" style="width:200px;height:200px;margin:10px;">'
					$a+='<a href="/api_image_download'+datas[i].url+'" style="display:block;">图片'+i+'下载</a>'
				};
				$("#content").html(imgs);
				$("#a_").html($a);
			}, 
			error : function(e)
			{ 
				console.log(e); 
			} 
		}) 
 	})

<% include('header') %>
 	<ul>
  		<% for(var i=0;i<data.length;i++){ %>
			<li>
				<span><%=data[i].name %></span>
				<span><%=data[i].age %></span>
				<span><%=data[i].city %></span>
			</li>
  		<% } %>
    </ul>
    <ul id="ul_user">
  		
 	</ul>
<% include('footer') %>

 <script src="/javascripts/jquery.min.js" type="text/javascript"></script>
 <script>
 	var handle=function(data){
 		var htmls="";
 		$.each(data,function(n,i){
 			console.log(i)
 			htmls+="<li><span>"+i.id+"</span><span>"+i.qq+"</span><span>"+i.email+"</span><span>"+i.address+"</span></li>"
 		})
 		$("#ul_user").html(htmls)
 	}
 	var aj = $.ajax( {    
	    url:'/api_easymysql',// 跳转到 action    
	    data:{    
	            
	    },    
	    type:'get',    
	    cache:false,    
	    dataType:'json',    
	    success:function(data) {    
	        handle(data)
	     },    
	     error : function() {    
	          
	     }    
	});  

  
 </script>